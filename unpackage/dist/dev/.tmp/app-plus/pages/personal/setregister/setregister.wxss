
.xxli {
	background-color: #fff;
	padding: 25px 10px;
	border-radius: 3px;
	position: relative;
}
.xxlia-span1 {
	color: #333;
}
.xxli:after {
	height: 1px;
	content: '';
	width: 100%;
	border-top: 1px solid #ebebeb;
	position: absolute;
	bottom: -1px;
	right: 0;
	-ms-transform: scaleY(0.5);
	    transform: scaleY(0.5);
	-webkit-transform: scaleY(0.5);
	z-index: 10;
}
.dingyue {
	background-color: transparent;
	width: 47.5px;
	height: 24.5px;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100%;
	border: none;
	float: right;
	margin-right: 3px;
	margin-top: -3px;
	outline: none;
}
.dingyue2 {
	background-image: url(data:image/png;base64,) !important;
