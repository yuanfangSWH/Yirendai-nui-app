
.tui-head-box {
	text-align: center;
}
.tui-head-box ._span {
	text-decoration: none;
	padding: 10px 4px;
	margin: 0 10px;
	display: inline-block;
	color: #515151;
	font-size: 14px;
}
.tui-head-box ._span.active {
	border-bottom: 2px solid #4e90e6;
	color: #4e90e6;
}
.banner-hb-img {
	width: 550rpx;
	margin: auto;
	text-align: center;
}
.banner-hb-img ._img {
	width: 100%;
	height: 1000rpx;
	display: inline-block;
	vertical-align: top;
	margin-top: 6%;
}
.wm-p {
	color: #98a7b8;
	text-align: center;
}
.wm-p ._p {
	margin: 0;
	margin-top: 6%;
}

